{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "quotes": [
      "warn",
      "single",
      { "avoidEscape": true, "allowTemplateLiterals": false }
    ],
    "import/prefer-default-export": "off",
    "no-unused-expressions": "warn",
    "import/no-cycle": "warn"
  }
}